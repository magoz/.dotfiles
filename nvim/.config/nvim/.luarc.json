{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "?.lua",
    "?/init.lua"
  ],
  "diagnostics": {
    "globals": ["vim"]
  },
  "workspace": {
    "checkThirdParty": false,
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library",
      "${3rd}/luassert/library"
    ]
  },
  "telemetry.enable": false
}